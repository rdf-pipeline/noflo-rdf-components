###########################################################
#
# List patient IDs
#
# This query depends on metadata in mGraph: 

PREFIX meta: <urn:meta#>
PREFIX mGraph: <urn:local:mGraph>

select distinct ?patientId { 
  graph mGraph: { 
    ?s a meta:Graph ;
      meta:patientId ?patientId .
  } }

