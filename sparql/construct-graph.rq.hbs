# construct-graph.rq.hbs

# This file uses http://handlebarsjs.com/
# The resulting document is SPARQL Query (tested with AllegroGraph)

CONSTRUCT {
    ?s ?p ?o
} WHERE {
    {{#if graph-uri~}}
        GRAPH <{{{graph-uri}}}>{
    {{~/if}}
        ?s ?p ?o
    {{#if graph-uri~}}
        }
    {{~/if}}
}
