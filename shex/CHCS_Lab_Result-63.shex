PREFIX chcs: <http://hokukahu.com/schema/chcss#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX map: <http://shex.io/extensions/Map/#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX fm: <http://hokukahu.com/map/>
BASE <http://hokukahu.com/shapes/>

start = @<Lab_Result-63>

<Lab_Result-63> {                             # <63-000007>
  a [chcs:Lab_Result-63];
  chcs:patient-63 @<Patient-2>;                                                      # <2-000007>
  chcs:clinical_chemistry-63 @<Clinical_Chemistry-63_04>+                            # <63_04-9999999.1111_000007>
}

<Patient-2> {                                 # <2-000007>
  a                                   [chcs:Patient-2];
  chcs:identifier                   LITERAL  %map:{ fm:patient.identifier %};        # "2-000007"
  rdfs:label                          LITERAL;                                         # "BUNNY,BUGS"
}

<Clinical_Chemistry-63_04> {                  # <63_04-9999999.1111_000007>
  a                                   [chcs:Clinical_Chemistry-63_04];
  chcs:identifier                   LITERAL  %map:{ fm:order.identifier %};          # "63_04-9999999.1111_000007"
  chcs:login_date_time-63_04        LITERAL  %map:{ fm:apparentDate %};              # "1990-01-01T00:00:00Z"
  chcs:specimen-63_04 {                                                              # <61-70>
    rdfs:label                        LITERAL  %map:{ fm:specimen.type %} }?;          # "BLOOD"
  chcs:collection_sample-63_04 {                                                     # <62-3>
    rdfs:label                        LITERAL  %map:{ fm:container.type %} }?;         # "HIV TUBE"
  chcs:date_time_specimen_taken-63_04 xsd:dateTime %map:{ fm:specimen.receivedTime %}; # "1990-01-01T00:00:00"^^xsd:dateTime
  chcs:hcp-63_04 {                                                                   # <6-485>
    rdfs:label                        LITERAL  %map:{ fm:collection.collector %} };    # "POOH,WINNIE"
  chcs:lab_work_element-63_04 {                                                      # <44-966>
    rdfs:label                        LITERAL  %map:{ fm:diagrpt.performer %} };       # "LABORATORY BMC 0000"
  chcs:result-63_04                 @<Result-63_07>+                                 # <11_07-4103_9999999.1111_000007>
}

<Result-63_07> {                              # <11_07-4103_9999999.1111_000007>
  a                                   [chcs:Result-63_07];
  chcs:identifier                   LITERAL  %map:{ fm:order.identifier %};          # "11_07-4103_9999999.1111_000007"
  chcs:test-63_07 {                                                                  # <60-4103>
    rdfs:label                        LITERAL  %map:{ fm:Observation.code %} };        # "INPROCESSING RH"
  chcs:lab_method-63_07 {                                                            # <8710-2037>
    rdfs:label                        LITERAL  %map:{ fm:Observation.code2 %} };       # "INPROCESSING RH"
  chcs:enter_date_time-63_07        LITERAL  %map:{ fm:diagRpt.issued %};            # "1990-01-01T00:00:08"^^xsd:dateTime
}

